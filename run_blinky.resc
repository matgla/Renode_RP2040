$platform_file=$ORIGIN/raspberry_blinky.repl
$global.FIRMWARE=$ORIGIN/blinky
include $ORIGIN/run_firmware.resc

logLevel -1 sysbus.gpio.led1
logLevel -1 sysbus.gpio.led2

machine StartGdbServer 3333 true sysbus.cpu0
